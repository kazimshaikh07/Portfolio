{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/static/admin/(.*)", "dest": "/portfolio/staticfiles/admin/$1" },
    { "src": "/static/(.*)", "dest": "/portfolio/static/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}

