{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/simple_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/django_handler.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/direct_django.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/minimal_django.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/simple_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "api/vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "portfolio/staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/portfolio/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/portfolio/media/$1"
    },
    {
      "src": "/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/simple",
      "dest": "/api/simple_wsgi.py"
    },
    {
      "src": "/django",
      "dest": "/api/django_handler.py"
    },
    {
      "src": "/direct",
      "dest": "/api/direct_django.py"
    },
    {
      "src": "/minimal",
      "dest": "/api/minimal_django.py"
    },
    {
      "src": "/app",
      "dest": "/api/simple_app.py"
    },
    {
      "src": "/portfolio",
      "dest": "/api/vercel_app.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "portfolio.settings",
    "VERCEL": "1",
    "PYTHONPATH": ".:./portfolio",
    "DEBUG": "False"
  },
  "buildCommand": "python -m pip install -r requirements-vercel.txt && cd portfolio && python manage.py collectstatic --noinput",
  "outputDirectory": "portfolio/staticfiles"
}