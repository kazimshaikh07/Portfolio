{
  "version": 2,
  "builds": [
    {
      "src": "portfolio/vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "portfolio/vercel.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "portfolio.settings",
    "VERCEL": "1"
  }
}
