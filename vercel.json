{
  "version": 2,
  "builds": [
    {
      "src": "portfolio/portfolio/vercel.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "headers": { "Cache-Control": "public, max-age=31536000, immutable" }, "dest": "/portfolio/static/$1" },
    { "src": "/(.*)", "dest": "portfolio/portfolio/vercel.py" }
  ]
}

